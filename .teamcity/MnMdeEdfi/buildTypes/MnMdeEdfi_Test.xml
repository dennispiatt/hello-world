<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="779296c8-c2d0-4ead-993b-18bbf2c59b6a" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>test</name>
  <description>Build Web API, Sandbox Admin, Swagger UI, ODS Databases</description>
  <settings>
    <options>
      <option name="cleanBuild" value="true" />
    </options>
    <parameters>
      <param name="adminApp.MajorVersion" value="2" />
      <param name="adminApp.MinorVersion" value="2" />
      <param name="adminApp.version" value="%adminApp.MajorVersion%.%adminApp.MinorVersion%.%build.counter%" />
      <param name="env.Nuget_ApiKey" value="%nexus.nuget.api.key%" />
      <param name="env.Nuget_Source" value="%nexus.nuget.package.source%" />
      <param name="odsapi.build.runDotnetTest" value="False" />
      <param name="odsapi.build.runPester" value="False" />
      <param name="odsapi.build.runPostman" value="False" />
      <param name="odsapi.build.runSmokeTest" value="False" />
      <param name="school.year" value="2022" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_17" name="Calculate admin app version" type="CalculatePackageVersion32_svn">
        <parameters>
          <param name="cpv.Channel" value="%Channel%" />
          <param name="cpv.DefaultTrunkName" value="%DefaultTrunkName%" />
          <param name="cpv.MajorVersion" value="%adminApp.MajorVersion%" />
          <param name="cpv.MinorVersion" value="%adminApp.MinorVersion%" />
          <param name="cpv.OverridePackageVersion" value="false" />
          <param name="cpv.PackageVersion" value="%adminApp.version%" />
          <param name="cpv.PatchVersion" value="%PatchPackageVersion%" />
          <param name="cpv.SemVerVersion" value="%SemVerVersion%" />
          <param name="cpv.SetBuildNumber" value="true" />
          <param name="cpv.TrunkName" value="%TrunkName%" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_18" name="Calculate api version" type="CalculatePackageVersion32_svn">
        <parameters>
          <param name="cpv.Channel" value="%Channel%" />
          <param name="cpv.DefaultTrunkName" value="%DefaultTrunkName%" />
          <param name="cpv.MajorVersion" value="%MajorPackageVersion%" />
          <param name="cpv.MinorVersion" value="%MinorPackageVersion%" />
          <param name="cpv.OverridePackageVersion" value="false" />
          <param name="cpv.PackageVersion" value="%PackageVersion%" />
          <param name="cpv.PatchVersion" value="%PatchPackageVersion%" />
          <param name="cpv.SemVerVersion" value="%SemVerVersion%" />
          <param name="cpv.SetBuildNumber" value="true" />
          <param name="cpv.TrunkName" value="%TrunkName%" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="MnMdeEdfi_MndoeTrunk">
        <checkout-rule rule="+:%TrunkName%/Ed-Fi-ODS =&gt; Ed-Fi-ODS" />
        <checkout-rule rule="+:%TrunkName%/Ed-Fi-ODS-AdminApp =&gt; Ed-Fi-ODS-AdminApp" />
        <checkout-rule rule="+:%TrunkName%/Ed-Fi-Ods-Docker =&gt; Ed-Fi-Ods-Docker" />
        <checkout-rule rule="+:%TrunkName%/Ed-Fi-ODS-Implementation =&gt; Ed-Fi-ODS-Implementation" />
        <checkout-rule rule="+:%TrunkName%/Ed-Fi-Standard =&gt; Ed-Fi-Standard" />
      </vcs-entry-ref>
    </vcs-settings>
    <requirements>
      <equals id="RQ_2" name="env.OS" value="Windows_NT" />
    </requirements>
    <build-triggers />
    <cleanup />
  </settings>
</build-type>

